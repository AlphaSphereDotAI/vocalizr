{
    "servers": {
        "Snyk": {
            "args": [
                "-y",
                "snyk@latest",
                "mcp",
                "-t",
                "stdio"
            ],
            "command": "npx",
            "type": "stdio"
        },
        "byterover-mcp": {
            "headers": {
                "Authorization": "Bearer ${env:BYTEROVER}"
            },
            "type": "http",
            "url": "https://mcp.byterover.dev/v2/mcp"
        },
        "docfork": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "docfork@latest"
            ]
        }
    }
}